          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "default": {
            "description": "Default fallback response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "deleted": {
                      "oneOf": [
                        {},
                        {}
                      ]
                    }
                  },
                  "required": [
                    "deleted"
                  ]
                }
