{
  "name": "@rcmade/hono-docs",
  "version": "1.0.30",
  "description": "Auto-generate OpenAPI 3.0 spec from Hono route types",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/rcmade/hono-docs.git"
  },
  "bugs": {
    "url": "https://github.com/rcmade/hono-docs/issues"
  },
  "homepage": "https://github.com/rcmade/hono-docs#readme",
  "keywords": [
    "openapi",
    "hono",
    "typescript",
    "documentation",
    "cli",
    "generator"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "hono-docs": "dist/cli/index.js"
  },
  "exports": {
    ".": "./dist/index.js",
    "./core": "./dist/core/index.js",
    "./cli": "./dist/cli/index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint . --ext .ts,.js",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:watch": "vitest tests/unit",
    "test:watch:all": "vitest",
    "prepare": "husky install"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "debug": "^4.4.1",
    "esbuild-register": "^3.6.0",
    "ts-morph": "^26.0.0",
    "tsx": "^4.20.3",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/debug": "^4.1.12",
    "@types/node": "^24.0.3",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "@vitest/coverage-v8": "3.2.4",
    "esbuild": "^0.25.5",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^16.2.0",
    "hono": "^4.9.4",
    "husky": "^8.0.0",
    "openapi-types": "^12.1.3",
    "standard-version": "^9.5.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1",
    "vitest": "^3.2.4"
  }
}
